<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="NeoPueblo.TramitesPage">
    

    <ContentPage.Content>
        <Grid>
            <BoxView Color="White" />
            <StackLayout>
                <Label Text="TRAMITES" TextColor="White"

                FontFamily="Cute Jellyfish.ttf#Cute Jellyfish" FontSize="35"
                   TranslationX="90" TranslationY="7"/>

                <Label x:Name="label_ejemplo" 
                FontFamily="Cute Jellyfish.ttf#Cute Jellyfish" FontSize="18"/>
                
                
                <Image Source="logorojoneo.png" VerticalOptions="Center" />
                <Label Text="COPYRIGHT-SH" TextColor="White"
                FontFamily="Cute Jellyfish.ttf#Cute Jellyfish" FontSize="18"
                   TranslationX="95" TranslationY="240"/>
                
            </StackLayout>
        </Grid>
    </ContentPage.Content>
</ContentPage>